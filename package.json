{
  "name": "escapp",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "start_heroku": "heroku local:start",
    "start_production": "sudo NODE_ENV=production node ./bin/www",
    "start_production_forever": "sudo forever start -c 'sudo npm run start_production' ./",
    "start_production_pm2": "sudo pm2 start ecosystem.config.js",
    "dev": "DEBUG=escapp  nodemon bin/www -e sass,js,json --ignore 'public/js/*,public/apps/*,migrations/*,seeders/*,test/*' --exec \"npm run css && npm run lint && node\" && xdg-open http://localhost:3000 ",
    "dev-windows": "nodemon bin/www -e sass,scss,js,json --ignore 'public/js/*,public/apps/*,migrations/*,seeders/*,test/*' --exec \"npm run css && npm run lint && node\" && start http://localhost:3000",
    "test": "DATABASE_URL=postgres://loguser:password@localhost/er_jest TEST=true jest --forceExit",
    "test:watch": "DATABASE_URL=postgres://loguser:password@localhost/er_jest TEST=true jest --watchAll",
    "create_local": "sequelize db:create --url $(grep DATABASE_URL  .env | cut -d '=' -f2)",
    "create_local_sqlite": "sequelize db:create --url sqlite://$(pwd)/escaperoom.sqlite",
    "create_local_sqlite_windows": "sequelize db:create --url sqlite://%cd%/escaperoom.sqlite",
    "migrate_local": "node db_migrate.js",
    "migrate_local_sqlite": "sequelize db:migrate --url sqlite://$(pwd)/escaperoom.sqlite",
    "migrate_local_sqlite_windows": "sequelize db:migrate --url sqlite://%cd%/escaperoom.sqlite",
    "migrate_heroku": "heroku run ./node_modules/.bin/sequelize db:migrate --url $DATABASE_URL",
    "undo_last_migration_local": "sequelize db:migrate:undo --url $(grep DATABASE_URL  .env | cut -d '=' -f2)",
    "undo_last_migration_local_sqlite": "sequelize db:migrate:undo --url sqlite://$(pwd)/escaperoom.sqlite",
    "seed_local": "sequelize db:seed:all --url $(grep DATABASE_URL  .env | cut -d '=' -f2)",
    "seed_local_sqlite": "sequelize db:seed:all --url sqlite://$(pwd)/escaperoom.sqlite",
    "seed_local_sqlite_windows": "sequelize db:seed:all --url sqlite://%cd%/escaperoom.sqlite",
    "seed_heroku": "heroku run ./node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "reset_heroku": "heroku pg:reset DATABASE",
    "reset_local_sqlite_windows": "rm -rf escaperoom.sqlite",
    "open_heroku": "heroku open",
    "css": "node-sass public/stylesheets/style.sass public/stylesheets/style.css",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "unused": "unused-exports --path ./ --ignore node_modules/ --ignore public --ignore migrations",
    "dump_heroku": "heroku pg:backups:download -o escape-room-upm.dump --app escape-room-upm"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "eslint": "^6.8.0",
    "express": "^4.16.4",
    "express-flash": "^0.0.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.17.1",
    "forever-monitor": "^2.0.0",
    "fs": "^0.0.1-security",
    "global": "^4.3.2",
    "http-errors": "~1.6.2",
    "i18n-express": "^1.1.3",
    "json-2-csv": "^3.7.6",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "node-sass-middleware": "0.11.0",
    "nodemailer": "^6.4.11",
    "nodemon": "^1.18.11",
    "pg": "7.10.0",
    "revalidator": "^0.3.1",
    "sass": "^1.26.10",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.4.0",
    "socket.io": "^2.4.0",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "uuid-token-generator": "^1.0.0",
    "zombie": "^6.1.4"
  },
  "devDependencies": {
    "jest": "^24.7.1",
    "sqlite3": "^4.2.0",
    "unused-exports": "^0.0.6-beta"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
